# chmod -R 755 $PWD/datahub-gms/log $PWD/datahub-gms/data $PWD/datahub-gms/config


# Mysql
MYSQL_HOST=mysql-container
MYSQL_PORT=3306
MYSQL_DATABASE=datahub
MYSQL_USER=datahub
MYSQL_PASSWORD=datahub

# Elasticsearch
ELASTICSEARCH_HOST=elasticsearch-container
ELASTICSEARCH_PORT=9200
DATAHUB_MAPPED_ELASTIC_PORT=9200

# Kafka
KAFKA_BOOTSTRAP_SERVER=kafka:29092 #broker
KAFKA_ZOOKEEPER_CONNECT=zk:2181

