# Network
DATAHUB_NETWORK_NAME=datahub_dev_network

# Image Versions Datahub

DATAHUB_ACTIONS_IMAGE=acryldata/datahub-actions
DATAHUB_ACTIONS_VERSION=head

DATAHUB_FRONTEND_IMAGE=acryldata/datahub-frontend-react
DATAHUB_FRONTEND_VERSION=head

DATAHUB_GMS_IMAGE=acryldata/datahub-gms
DATAHUB_GMS_VERSION=head

DATAHUB_UPGRADE_IMAGE=acryldata/datahub-upgrade
DATAHUB_UPGRADE_VERSION=head

DATAHUB_ELASTIC_SETUP_IMAGE=acryldata/datahub-elasticsearch-setup
DATAHUB_ELASTIC_SETUP_VERSION=head

DATAHUB_KAFKA_SETUP_IMAGE=acryldata/datahub-kafka-setup
DATAHUB_KAFKA_SETUP_VERSION=head

DATAHUB_MYSQL_SETUP_IMAGE=acryldata/datahub-mysql-setup
DATAHUB_MYSQL_SETUP_VERSION=head

DATAHUB_CONFLUENT_SCHEMA_REGISTRY_IMAGE=confluentinc/cp-schema-registry
DATAHUB_CONFLUENT_SCHEMA_REGISTRY_VERSION=7.4.0


# Ports
DATAHUB_MAPPED_FRONTEND_PORT=9003
DATAHUB_MAPPED_GMS_PORT=8080
DATAHUB_MAPPED_SCHEMA_REGISTRY_PORT=8081


